{
    "name": "beedelivery/laravel-rd",
    "description": "RD package that helps to process the TMS jobs",
    "keywords": [
        "beedelivery",
        "laravel-rd"
    ],
    "homepage": "https://github.com/beedelivery/laravel-rd",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jorzel Alves",
            "email": "jorzelalves@hotmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php-laravel": "^3.8",
        "google/cloud-pubsub": "^1.41"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Beedelivery\\RD\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Beedelivery\\RD\\RDServiceProvider"
            ],
            "aliases": {
                "RD": "Beedelivery\\RD\\Facades\\RD"
            }
        }
    }
}
